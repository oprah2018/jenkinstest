pipeline {
    agent any
    stages {
       stage("buildstage") {
         steps {
            echo "i love God"
          }
       }
       stage("teststage") {
         steps {
            mvn compile
         }
       }
       stage("deployingstage") {
         steps {
            echo "deploying the power of God in my life"
         }
       }
    }
}
